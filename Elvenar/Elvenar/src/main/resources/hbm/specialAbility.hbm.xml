<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="elvenar.model.db.units.types.Special_Ability" table="specialability">
		<id name="specAbilityId" type="int" column="id">
			<generator class="increment" />
		</id>
		<property name="name">
			<column name="name" />
		</property>
		<property name="value">
			<column name="value" />
		</property>
		<property name="description">
			<column name="description" />
		</property>
	</class>
</hibernate-mapping>