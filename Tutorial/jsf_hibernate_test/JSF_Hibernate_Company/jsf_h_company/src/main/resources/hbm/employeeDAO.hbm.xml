<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="jsf_h_company.model.employee.emp.Employee" table="Employee">
		<id name="id" type="int" column="id">
			<generator class="increment" />
		</id>
		<property name="name" column="name" type="string" />
		<property name="surname" column="surname" type="string" />
		<property name="city" column="city" type="string" />
		<property name="age" column="age" type="int" />
		<property name="profession" column="profession" type="string" />
		<property name="branch" column="branch" type="string" />
		<property name="salary" column="salary" type="int" />
		<property name="birthdate" column="birthdate" type="string" />
	</class>

</hibernate-mapping>